<?xml version="1.0" encoding="UTF-8"?>
<manifest>

   <remote name="github"
           fetch="https://github.com/"/>

   <remote name="private"
   		   fetch="ssh://git@github.com" />
	   
   <default revision="refs/heads/master"
            remote="private"
            sync-c="true"
	          sync-j="4" />
	
	<!-- <project name="ifdroids/APPNAME" path="APPNAME" /> -->
	<project name="ifdroids/CoordinatesConverter" path="coordinatesconverter"/>
	<project name="ifdroids/Scripty" path="scripty"/>
	<project name="ifdroids/NavigationDrawerSavedStateLib" path="navigationdrawersavedstatelib"/>
	
	<project name="ifdroids/manifest" path="manifest" />
	
	<project name="ifdroids/resources" path="resources" />
	<project name="ifdroids/terminal_scripts" path="terminal_scripts">
		<copyfile src="createremotes.sh" dest="createremotes.sh" />
	</project>
		

</manifest>
